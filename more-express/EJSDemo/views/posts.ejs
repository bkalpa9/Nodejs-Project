<h1>The Posts page</h1>
<% for (let i = 0; i < posts.length; i++) { %>
  <li>
    <%= posts[i].title %> - <strong> <%= posts[i].author%></strong>
    </li>
  <% } %>
<% posts.forEach(function (posts){ %>
  <li>
  <%= posts.title %> - <strong> <%= posts.author%></strong>
  </li>
<% }) %>
